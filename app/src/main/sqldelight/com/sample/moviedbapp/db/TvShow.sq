CREATE TABLE TvShow (
  dbId INTEGER PRIMARY KEY AUTOINCREMENT,
  id INTEGER NOT NULL,
  posterPath TEXT NOT NULL,
  backdropPath TEXT,
  name TEXT NOT NULL,
  overview TEXT NOT NULL,
  voteAverage REAL NOT NULL,

  CONSTRAINT unique_id UNIQUE (id)
);

insertOrIgnore:
INSERT OR IGNORE INTO TvShow
VALUES (?, ?, ?, ?, ?, ?, ?);

lastInsertRowId:
SELECT last_insert_rowid();

clearAll:
DELETE FROM TvShow;


